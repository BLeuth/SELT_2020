update_users = (data) ->
  console.log(data)
  $('#usernames').html(data)
  return

$ ->
  pusher = new Pusher('<%= Pusher.key %>')
  channel = pusher.subscribe('update_users_'+window.location.href.replace(/.+\/.+\//i, ''))
  channel.bind 'up_users', (data) ->
    update_users data
    return
#  pusher.disconnect();
  return