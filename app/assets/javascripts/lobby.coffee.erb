update_users = (data) ->
  console.log(data)
  $('#usernames').html(data)
  return

$ ->
  pusher = new Pusher('<%= ENV["PUSHER_KEY"] %>',
    cluster: '<%= ENV["PUSHER_CLUSTER"] %>',
    encrypted: true)
  channel = pusher.subscribe('update_users')
  channel.bind 'up_users', (data) ->
    update_users data
    return
  return